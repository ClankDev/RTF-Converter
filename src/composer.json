{
    "name": "clank-ai/rtf-converter",
    "description": "A simple RTF to Plain Text Converter using regular expressions. Support for PHP, Python, Javascript and CSharp",
    "type": "library",
    "version": "0.1.5",
    "license": "MIT",
    "require": {
        "php": ">=7.0"
    },
    "autoload": {
        "psr-4": {
            "RtfConverter\\": "src/php/"
        }
    },
    "extra": {
        "python-source": "src/python/",
        "javascript-source": "src/javascript/",
        "csharp-source": "src/csharp/"
    }
}